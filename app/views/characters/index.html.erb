<div class="page-header">
  <h1>Crew <small> of the Voidfeather</small></h1>
</div>

<div class="panel panel-default">
  <div class="panel-body">
    <ul class='list-group'>
      <% @characters.each do |character| %>
      <li class='list-group-item'>
        <%= link_to character.name, character_path(character.name) %>
        <small>
          of <%= character.race %>
        </small>
      </li>
      <% end %>
    </ul>
  </div>
</div>
